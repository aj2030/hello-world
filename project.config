[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = A hello world repository
[access "refs/*"]
	owner = group user/aj2030
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/aj2030
	label-Verified = -1..+1 group user/aj2030
	push = group user/aj2030
	read = group user/aj2030
	rebase = group user/aj2030
	removeReviewer = group user/aj2030
	submit = group user/aj2030
[access "refs/heads/*"]
	create = group user/aj2030
	push = +force group user/aj2030
	pushMerge = group user/aj2030
	read = group user/aj2030
[access "refs/tags/*"]
	push = group user/aj2030
	push = +force group user/aj2030
